/** 
 * Multiplexor:
 * if (sel = 0) out = a, else out = b
 */
CHIP Mux {
    IN a, b, sel;
    OUT out;

    PARTS:
    Not(in=sel, out=notsel);
    Or(a=notsel, b=b, out=or1);
    Or(a=sel, b=a, out=or2);
    And(a=or1, b=or2, out=out);
}
